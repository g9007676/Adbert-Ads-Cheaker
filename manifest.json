{
    "manifest_version": 2,
    "name": "AdChecker",
    "version": "1.0",
    "description": "check GA | Adwords | FB pixel code Run correctly",

    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "tabs"
